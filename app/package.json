{
  "name": "my-skeleton-app",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
