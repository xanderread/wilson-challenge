  "dependencies": {
    "@floating-ui/dom": "1.6.12",
    "@types/webrtc": "^0.0.44",
    "chromadb": "^1.9.4",
    "chromadb-default-embed": "^2.13.2",
    "dotenv": "^16.4.5",
    "install": "^0.13.0",
    "llamaindex": "^0.8.0",
    "npm": "^10.9.1",
    "pspdfkit": "^2024.8.0",
    "svelte-pdf": "^1.0.20",
    "svelte-typewriter": "^3.2.3"
